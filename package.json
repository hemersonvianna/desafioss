{
  "name": "carousel-smarttv",
  "version": "0.0.2",
  "description": "Component of Carousel for TV with React.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxsarena/carousel-smarttv-angular-react.git"
  },
  "keywords": [
    "react",
    "es6",
    "babel",
    "webpack",
    "karma"
  ],
  "main": "source/assets/js/app.js",
  "scripts": {
    "preinstall": "npm install -g bower grunt-cli",
    "postinstall": "bower install && grunt",
    "start": "grunt",
    "test": "npm run lint && grunt test",
    "clean": "rm -rf public",
    "lint": "eslint config source/assets/js/** --ext .js",
    "precommit": "npm run lint",
    "prepush": "npm run lint",
    "build:dev": "set NODE_ENV=development && webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress",
    "build": "set NODE_ENV=production && grunt",
    "doc": "esdoc -c esdoc.json"
  },
  "author": "Hemerson Vianna <hemerson.lourenco@gmail.com> (https://nerdcalistenico.com.br)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sxsarena/carousel-smarttv-angular-react/issues"
  },
  "homepage": "https://github.com/sxsarena/carousel-smarttv-angular-react#readme",
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-addons-test-utils": "^15.1.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^5.2.1",
    "grunt": ">=0.4.5",
    "grunt-autoprefixer": "^3.0.3",
    "grunt-combine-media-queries": "~1.0.0",
    "grunt-contrib-clean": "^0.7.0",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-cssmin": "^0.12.2",
    "grunt-contrib-htmlmin": "^0.6.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-eslint": "^18.1.0",
    "grunt-karma": "^2.0.0",
    "grunt-sass": "^1.0.0",
    "grunt-webpack": "^1.0.11",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^1.0.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack": "^1.7.0",
    "load-grunt-config": "^0.19.1",
    "mocha": "^2.5.3",
    "node-sass": "^4.14.1",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "watchify": "^3.7.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
